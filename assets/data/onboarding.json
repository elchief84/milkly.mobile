{
    "version": 1,
    "flowId": "breastfeeding_onboarding_v1",
    "steps": [
      {
        "id": "childProfile",
        "titleKey": "bfOnboarding_questionnaire_step_childProfile_title",
        "questions": [
          {
            "id": "childName",
            "type": "text",
            "required": true
          },
          {
            "id": "childBirthDate",
            "type": "date",
            "required": true
          },
          {
            "id": "childSex",
            "type": "single_choice",
            "required": false,
            "options": [
              { "id": "male" },
              { "id": "female" },
              { "id": "other" }
            ]
          },
          {
            "id": "childrenInThisBirth",
            "type": "single_choice",
            "required": true,
            "options": [
              { "id": "one" },
              { "id": "twins" },
              { "id": "moreThanTwo" }
            ]
          }
        ]
      },
      {
        "id": "feedingStatus",
        "titleKey": "bfOnboarding_questionnaire_step_feedingStatus_title",
        "questions": [
          {
            "id": "currentFeedingMode",
            "type": "single_choice",
            "required": true,
            "options": [
              { "id": "breastOnly" },
              { "id": "breastAndPumped" },
              { "id": "breastAndFormula" },
              { "id": "pumpedOnly" },
              { "id": "formulaOnly" },
              { "id": "other" }
            ]
          },
          {
            "id": "avgFeedsPerDay",
            "type": "number",
            "required": false,
            "min": 0
          },
          {
            "id": "bottleUsage",
            "type": "single_choice",
            "required": true,
            "options": [
              { "id": "never" },
              { "id": "sometimes" },
              { "id": "regular" }
            ]
          }
        ]
      },
      {
        "id": "pediatricPlan",
        "titleKey": "bfOnboarding_questionnaire_step_pediatricPlan_title",
        "questions": [
          {
            "id": "pediatricFeedingMode",
            "type": "single_choice",
            "required": true,
            "options": [
              { "id": "onDemand" },
              { "id": "intervals" },
              { "id": "mixed" },
              { "id": "notDefined" }
            ]
          },
          {
            "id": "dayIntervalDescription",
            "type": "text",
            "required": false,
            "visibilityCondition": {
              "questionId": "pediatricFeedingMode",
              "operator": "in",
              "value": ["intervals", "mixed"]
            }
          },
          {
            "id": "nightIntervalType",
            "type": "single_choice",
            "required": false,
            "options": [
              { "id": "sameAsDay" },
              { "id": "different" },
              { "id": "notDefined" }
            ],
            "visibilityCondition": {
              "questionId": "pediatricFeedingMode",
              "operator": "in",
              "value": ["intervals", "mixed"]
            }
          },
          {
            "id": "nightMaxIntervalHours",
            "type": "number",
            "required": false,
            "min": 0,
            "visibilityCondition": {
              "questionId": "nightIntervalType",
              "operator": "equals",
              "value": "different"
            }
          },
          {
            "id": "maxIntervalAnytimeDefined",
            "type": "single_choice",
            "required": false,
            "options": [
              { "id": "yes" },
              { "id": "no" },
              { "id": "dontKnow" }
            ],
            "visibilityCondition": {
              "questionId": "pediatricFeedingMode",
              "operator": "in",
              "value": ["onDemand", "mixed"]
            }
          },
          {
            "id": "maxIntervalAnytimeHours",
            "type": "number",
            "required": false,
            "min": 0,
            "visibilityCondition": {
              "questionId": "maxIntervalAnytimeDefined",
              "operator": "equals",
              "value": "yes"
            }
          }
        ]
      },
      {
        "id": "feedTimersAndBreast",
        "titleKey": "bfOnboarding_questionnaire_step_feedTimersAndBreast_title",
        "questions": [
          {
            "id": "trackLastBreast",
            "type": "single_choice",
            "required": true,
            "options": [
              { "id": "yes" },
              { "id": "no" }
            ]
          },
          {
            "id": "breastPattern",
            "type": "single_choice",
            "required": false,
            "options": [
              { "id": "onePerFeed" },
              { "id": "bothPerFeed" },
              { "id": "depends" }
            ]
          },
          {
            "id": "feedTimerUsage",
            "type": "single_choice",
            "required": true,
            "options": [
              { "id": "fullTimer" },
              { "id": "approx" },
              { "id": "no" }
            ]
          },
          {
            "id": "nextFeedReminder",
            "type": "single_choice",
            "required": true,
            "options": [
              { "id": "before" },
              { "id": "onTime" },
              { "id": "no" }
            ]
          }
        ]
      },
      {
        "id": "pumping",
        "titleKey": "bfOnboarding_questionnaire_step_pumping_title",
        "visibilityCondition": {
          "questionId": "currentFeedingMode",
          "operator": "in",
          "value": ["breastAndPumped", "pumpedOnly", "breastAndFormula"]
        },
        "questions": [
          {
            "id": "pediatricPumpingScheduleDefined",
            "type": "single_choice",
            "required": true,
            "options": [
              { "id": "yes" },
              { "id": "no" }
            ]
          },
          {
            "id": "pumpingTimes",
            "type": "multi_choice",
            "required": false,
            "options": [
              { "id": "morning" },
              { "id": "afternoon" },
              { "id": "eveningNight" },
              { "id": "other" }
            ],
            "visibilityCondition": {
              "questionId": "pediatricPumpingScheduleDefined",
              "operator": "equals",
              "value": "yes"
            }
          },
          {
            "id": "pumpingTimesNotes",
            "type": "text",
            "required": false,
            "visibilityCondition": {
              "questionId": "pediatricPumpingScheduleDefined",
              "operator": "equals",
              "value": "yes"
            }
          },
          {
            "id": "pumpingReminders",
            "type": "single_choice",
            "required": true,
            "options": [
              { "id": "onTime" },
              { "id": "before" },
              { "id": "no" }
            ]
          },
          {
            "id": "trackPumpedAmount",
            "type": "single_choice",
            "required": true,
            "options": [
              { "id": "yes" },
              { "id": "no" }
            ]
          }
        ]
      },
      {
        "id": "night",
        "titleKey": "bfOnboarding_questionnaire_step_night_title",
        "questions": [
          {
            "id": "nightNotifications",
            "type": "single_choice",
            "required": true,
            "options": [
              { "id": "yes" },
              { "id": "onlyIfExceeds" },
              { "id": "no" }
            ]
          },
          {
            "id": "nightIntervalOverrideHours",
            "type": "number",
            "required": false,
            "min": 0,
            "visibilityCondition": {
              "questionId": "nightNotifications",
              "operator": "in",
              "value": ["yes", "onlyIfExceeds"]
            }
          }
        ]
      },
      {
        "id": "otherChildren",
        "titleKey": "bfOnboarding_questionnaire_step_otherChildren_title",
        "questions": [
          {
            "id": "configureOtherChildren",
            "type": "single_choice",
            "required": true,
            "options": [
              { "id": "twin" },
              { "id": "otherChild" },
              { "id": "no" }
            ]
          },
          {
            "id": "copySettingsForOtherChild",
            "type": "single_choice",
            "required": false,
            "options": [
              { "id": "copy" },
              { "id": "new" }
            ],
            "visibilityCondition": {
              "questionId": "configureOtherChildren",
              "operator": "in",
              "value": ["twin", "otherChild"]
            }
          }
        ]
      }
    ]
  }  